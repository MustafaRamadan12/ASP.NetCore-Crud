@model ApplicationUser

@{
    ViewData["Title"] = "Edit";
}
<h1 >Edit</h1>
<br/>

<form class="form-group" asp-controller="Account" asp-action="Edit">

    <div asp-validation-summary="All" class="text-danger"></div>

    <label asp-for="Id">Id :</label>
    <input type="text" class="form-control" asp-for="Id" readonly/>
    <br/>
    <label asp-for="UserName">User name :</label>
    <input type="text" class="form-control" asp-for="UserName" />
    <span asp-validation-for="UserName" class="text-danger"></span>
    <br />

    
    <center>
        <input type="submit" value="Edit" class="btn btn-primary" />
        <input type="reset" value="Cancle" class="btn btn-danger" />
    </center>
    </form>
    
@section MicrosoftValidation{
    <script src="~/plugins/jquery-validation/jquery.validate.js"></script>
    <script src="~/plugins/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
}

