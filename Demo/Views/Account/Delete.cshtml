@model ApplicationUser

@{
    ViewData["Title"] = "Delete";
}
<h1>Delete</h1>
<br/>

<form class="form-group" asp-controller="Account" asp-action="Delete">

  
        <br />

    <center>
        <h2>Are You Sure To Delete This Data ? </h2>
    </center>

    <br />

    <label asp-for="Id">Id :</label>
    <input type="text" class="form-control" asp-for="Id" readonly/>
    <br/>
    <label asp-for="UserName">User name :</label>
    <input type="text" class="form-control" asp-for="UserName" readonly />
    
    <br />

    
    <center>
        <input type="submit" value="Delete" class="btn btn-primary" /> 
        <input asp-controller="Account" class="btn btn-warning" asp-action="Index" value="users"/>
        <input type="reset" value="Cancle" class="btn btn-danger" />
    </center>
    </form>
 

